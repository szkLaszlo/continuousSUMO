<!-- vehicle flow definition -->
<routes>
    <routeDistribution id="routedist1">
        <route id="r1"
               edges="A gneE1 gneE2 gneE3 gneE4 gneE5 gneE6 gneE7 gneE8 gneE9 gneE10 gneE11 gneE12 gneE13 gneE14 gneE15 gneE16 gneE17 gneE18 B"
               probability="1"/>
    </routeDistribution>
    <route id="r1"
           edges="A gneE1 gneE2 gneE3 gneE4 gneE5 gneE6 gneE7 gneE8 gneE9 gneE10 gneE11 gneE12 gneE13 gneE14 gneE15 gneE16 gneE17 gneE18 B"/>

    <vType id="car1" length="5.2" accel="4" decel="7" sigma="1" speedFactor="normc(0.8,0.1,0.8,1.2)" probability="0.2"
           lcCooperative="0" lcKeepRight="0.1" color="0.5, 0.5, 0.5"/>
    <vType id="car2" length="5.2" accel="5" decel="6" sigma="1" speedFactor="normc(0.8,0.1,0.7,0.9)" probability="0.2"
           lcCooperative="0" lcKeepRight="0.2" color="0, 0.5, 0.5"/>
    <vType id="car3" length="5" accel="5" decel="6" sigma="0.5" speedFactor="normc(0.8,0.2,0.6,0.9)" probability="0.2"
           lcCooperative="0" lcKeepRight="0.5" color="1, 0.5, 0.5"/>
    <vType id="car4" length="4.5" accel="5" decel="6" sigma="1" speedFactor="normc(0.8,0.1,0.65,0.95)" probability="0.2"
           lcCooperative="0" lcKeepRight="0.1" color="0.5, 1, 1"/>
    <vType id="car5" length="5" accel="5" decel="6" sigma="1" speedFactor="normc(0.8, 0.05, 0.75, 0.85)"
           probability="0.2"
           lcCooperative="0" lcKeepRight="0" color="0, 1, 0.5"/>

    <vType id="ego" length="5" accel="50" decel="60" sigma="1"/>

    <vTypeDistribution id="typedist1" vTypes="car1 car2 car3 car4"/>
    <vTypeDistribution id="typedist2" vTypes="car5"/>
    <flow id="carflow1" type="typedist1" route="r1" beg="0" end="3600" departLane="random" vehsPerHour="1500"/>
    <flow id="carflow2" type="typedist2" route="r1" beg="0" end="3600" departLane="random" vehsPerHour="800"/>
</routes>
